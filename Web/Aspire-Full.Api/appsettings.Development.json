{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "aspiredb": "Host=localhost;Port=5432;Database=aspire;Username=aspire;Password=aspire_dev_password"
  },
  "DockerRegistry": {
    "BaseAddress": "http://localhost:5000/",
    "AllowInsecureTls": true,
    "Patterns": {
      "RepositoryTemplate": "aspire/{service}-{environment}",
      "TagTemplate": "{version}-{architecture}",
      "Namespace": "aspire",
      "DefaultEnvironment": "dev",
      "DefaultArchitecture": "linux-x64",
      "DefaultVersion": "1.0.0",
      "SampleServices": ["api", "web", "workers", "subagents"]
    }
  },
  "TensorModels": {
    "Models": [
      {
        "Id": "local-clip-text",
        "DisplayName": "Local CLIP Text",
        "Description": "Developer-friendly CLIP tower",
        "Repository": "ghcr.io/aspire-full/clip-text",
        "Tag": "dev",
        "Digest": "sha256:aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa",
        "ModelUri": "http://localhost:5173/models/clip-text.onnx",
        "DocumentationUri": "http://localhost:5173/docs/models/clip-text",
        "EmbeddingSize": 512,
        "InputShape": [1, 32, 512],
        "PreferredExecutionProviders": ["webgpu", "webgl2"],
        "AlternateExecutionProviders": ["wasm-simd"]
      }
    ]
  },
  "Qdrant": {
    "Endpoint": "http://localhost:6334",
    "Collection": "aspire-full-vectors-dev",
    "VectorSize": 512,
    "GrpcTimeoutSeconds": 30
  }
}
