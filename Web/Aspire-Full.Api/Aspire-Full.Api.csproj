  <ItemGroup>
    <ProjectReference Include="..\..\Infra\Aspire-Full.ServiceDefaults\Aspire-Full.ServiceDefaults.csproj" />
    <ProjectReference Include="..\..\Core\Aspire-Full.Shared\Aspire-Full.Shared.csproj" />
  </ItemGroup>

</Project>
