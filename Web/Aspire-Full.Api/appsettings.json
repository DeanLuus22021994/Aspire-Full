{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DockerRegistry": {
    "BaseAddress": "http://localhost:5000/",
    "CatalogPageSize": 100,
    "EnableOfflineFallback": true,
    "Patterns": {
      "RepositoryTemplate": "aspire/{service}-{environment}",
      "TagTemplate": "{version}-{architecture}",
      "Namespace": "aspire",
      "DefaultEnvironment": "dev",
      "DefaultArchitecture": "linux-x64",
      "DefaultVersion": "1.0.0",
      "SampleServices": ["api", "web", "workers"]
    }
  },
  "TensorModels": {
    "Models": [
      {
        "Id": "docs-clip-text",
        "DisplayName": "CLIP Text Encoder (Docs)",
        "Description": "Smaller text tower for docs authoring.",
        "Repository": "ghcr.io/aspire-full/clip-text",
        "Tag": "docs",
        "Digest": "sha256:1111111111111111111111111111111111111111111111111111111111111111",
        "ModelUri": "https://cdn.aspire-full.example/models/docs/clip-text.onnx",
        "DocumentationUri": "https://docs.aspire-full.example/models/clip-text",
        "EmbeddingSize": 512,
        "InputShape": [1, 32, 512],
        "PreferredExecutionProviders": ["webgpu", "webgl2"],
        "AlternateExecutionProviders": ["wasm-simd"]
      },
      {
        "Id": "uat-multimodal",
        "DisplayName": "UAT Multimodal Encoder",
        "Description": "Validation multimodal encoder with quantized weights.",
        "Repository": "ghcr.io/aspire-full/multimodal",
        "Tag": "uat",
        "Digest": "sha256:2222222222222222222222222222222222222222222222222222222222222222",
        "ModelUri": "https://uat-assets.aspire-full.example/models/multimodal-uat.onnx",
        "DocumentationUri": "https://uat.aspire-full.example/docs/models/multimodal",
        "EmbeddingSize": 768,
        "InputShape": [1, 16, 1024],
        "PreferredExecutionProviders": ["webgpu"],
        "AlternateExecutionProviders": ["webgl2", "wasm-simd"]
      }
    ]
  },
  "Qdrant": {
    "Endpoint": "http://qdrant:6334",
    "Collection": "aspire-full-vectors",
    "VectorSize": 1024,
    "GrpcTimeoutSeconds": 30
  }
}
