{
  "inputs": [
    {
      "id": "aspireMcpApiKey",
      "type": "promptString",
      "description": "Aspire Dashboard API Key",
      "default": "",
      "password": true
    },
    {
      "id": "githubMcpApiKey",
      "type": "promptString",
      "description": "GitHub MCP API Key",
      "default": "aspire-github-mcp",
      "password": true
    }
  ],
  "servers": {
    "aspire-dashboard": {
      "type": "http",
      "url": "http://localhost:16036",
      "headers": {
        "x-mcp-api-key": "${input:aspireMcpApiKey}"
      }
    },
    "github-mcp": {
      "type": "http",
      "url": "http://localhost:17071/mcp",
      "headers": {
        "x-mcp-api-key": "${input:githubMcpApiKey}"
      }
    }
  }
}
