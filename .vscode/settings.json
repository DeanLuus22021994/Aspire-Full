{
  "dotnet.defaultSolution": "Aspire-Full.slnx",
  "omnisharp.enableRoslynAnalyzers": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "files.exclude": {
    "**/bin": true,
    "**/obj": true
  },
  "python.envFile": "${workspaceFolder}/.config/python/.env",
  "github.copilot.chat.codeGeneration.useReferencedFiles": true,
  "github.copilot.chat.codesearch.enabled": true,
  "dotnet.completion.showCompletionItemsFromUnimportedNamespaces": true,
  "files.associations": {
    ".pycodestyle": "ini"
  },
  "cSpell.words": [
    "addopts",
    "ASPNETCORE",
    "AUTHMODE",
    "CERTDIR",
    "chelnak",
    "CUDA",
    "Dind",
    "Instrumentor",
    "llms",
    "nektos",
    "pgvector",
    "pylint",
    "pytest",
    "qdrant",
    "seachicken",
    "Serilog",
    "triaging",
    "xmldoc"
  ],
  "python.defaultInterpreterPath": "${workspaceFolder}/AI/Aspire-Full.Python/python-agents/.venv/Scripts/python.exe",
  "cmake.sourceDirectory": "${workspaceFolder}/AI/Aspire-Full.Tensor/Native",
  "nsight.server.connection.port": 4500,
  "nsight.server.connection.host": "localhost",
  "nsight.compute.path": "C:\\Program Files\\NVIDIA Corporation\\Nsight Compute 2025.2.1\\host\\windows-desktop-win7-x64\\ncu-ui.exe",
  "nsight.systems.path": "C:\\Program Files\\NVIDIA Corporation\\Nsight Systems 2025.6.1\\host-windows-x64\\nsys-ui.exe",
  "python.analysis.typeCheckingMode": "strict",
  "python.analysis.diagnosticSeverityOverrides": {
    "reportUnusedImport": "warning",
    "reportUnusedVariable": "warning",
    "reportUnusedParameter": "none",
    "reportMissingParameterType": "none",
    "reportUnknownMemberType": "none",
    "reportUnknownParameterType": "none",
    "reportUnknownVariableType": "none",
    "reportUnknownArgumentType": "none",
    "reportUnnecessaryTypeIgnoreComment": "none",
    "reportPrivateUsage": "none"
  },
  "pylint.enabled": false,
  "flake8.enabled": false,
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true
  }
}
