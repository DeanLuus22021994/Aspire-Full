{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "aspiredb": "Host=localhost;Port=5432;Database=aspire;Username=aspire;Password=aspire_dev_password"
  },
  "DockerRegistry": {
    "BaseAddress": "http://localhost:5000/",
    "AllowInsecureTls": true,
    "Patterns": {
      "RepositoryTemplate": "aspire/{service}-{environment}",
      "TagTemplate": "{version}-{architecture}",
      "Namespace": "aspire",
      "DefaultEnvironment": "dev",
      "DefaultArchitecture": "linux-x64",
      "DefaultVersion": "1.0.0",
      "SampleServices": ["api", "web", "workers", "subagents"]
    }
  }
}
